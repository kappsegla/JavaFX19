<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToggleButton?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Circle?>

<BorderPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0"
            prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="sample.Controller">
   <left>
      <VBox alignment="TOP_CENTER" maxWidth="200.0" minHeight="100.0" minWidth="100.0" prefHeight="200.0"
            prefWidth="100.0" spacing="20.0">
         <BorderPane.margin>
            <Insets top="10.0"/>
         </BorderPane.margin>
         <Label text="Label"/>
         <Button fx:id="button1" mnemonicParsing="false" onAction="#button1Action" onKeyPressed="#keyPressed"
                 text="Button"/>
         <CheckBox fx:id="checkBox1" mnemonicParsing="false" text="CheckBox"/>
         <ToggleButton fx:id="togglebutton" mnemonicParsing="false" text="ToggleButton"/>
      </VBox>
   </left>
   <center>
      
   </center>
   <center>
      <GridPane BorderPane.alignment="CENTER">
         <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
         </columnConstraints>
         <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
         </rowConstraints>
         <TextField fx:id="textField" promptText="Text goes here"/>
         <TextArea fx:id="textArea" prefHeight="200.0" prefWidth="200.0" promptText="Enter some text"
                   GridPane.rowIndex="1"/>
         <Slider GridPane.rowIndex="2"/>
         <Circle fill="DODGERBLUE" onMouseClicked="#circleClicked" radius="50.0" stroke="BLACK" strokeType="INSIDE"
                 GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="1"
                 GridPane.valignment="CENTER">
            <cursor>
               <Cursor fx:constant="HAND"/>
            </cursor>
         </Circle>
      </GridPane>
   </center>
</BorderPane>
